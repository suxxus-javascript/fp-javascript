{
  "name": "fp-javascript",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/suxxus-studies/fp-javascript"
  },
  "author": "suxxus",
  "license": "ISC",
  "description": "functional-paradigm-studies",
  "scripts": {
    "lint": "eslint --color  src || true",
    "test": "NODE_PATH=src tape src/tests | tap-colorize",
    "dev": "watch -p 'src/**/*.js' -c 'npm test & npm run lint'",
    "check": "npm outdated --depth=0",
    "update": "updtr"
  },
  "devDependencies": {
    "eslint": "3.7.1",
    "eslint-cli": "1.1.0",
    "eslint-config-eslint": "3.0.0",
    "tap-colorize": "1.2.0",
    "tape": "4.6.2",
    "updtr": "0.2.1",
    "watch-cli": "0.2.1"
  },
  "keywords": [
    "javascript", "studies", "fp", "functional paradigm"
  ]
}
